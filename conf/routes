# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

GET         /                      controllers.HomeController.index(request: Request)
POST        /                      controllers.HomeController.uploadFromWebPage(request: Request)

POST        /uploads               controllers.HomeController.fileUploadApi(request: Request)

GET         /view/:name            controllers.HomeController.viewDataset(name: String)
GET         /download/:name        controllers.HomeController.downloadDataset(name: String)

GET         /downloadapp           controllers.HomeController.downloadApp

GET         /test                  controllers.HomeController.test

# Map static resources from the /public folder to the /assets URL path
GET         /assets/*file          controllers.Assets.versioned(path="/public", file: Asset)
